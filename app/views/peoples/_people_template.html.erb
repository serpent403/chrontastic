<div class="panel panel-default event-template">
  <div class="media">
    <div class="media-left" style="float:left;padding:2px;">
      <a href="#">
        <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/png;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
      </a>
    </div>
    <div class="media-body" style="padding:2px;">
      <div class="media-heading">
        <span style="font-weight:bold;font-size:15px;padding-right:15px;">
          <%= p.username %>
        </span>
      </div>

      <div style="padding: 4px 0 4px 0;font-size: 11px;">
        <span class="glyphicon glyphicon-envelope" style="padding-right:15px;">
          <span style="font-family: Helvetica Neue, Helvetica, Arial, sans-serif;"><%= p.email %></span>
        </span>

      </div>

      <div>
      </div>

    </div> <!--media-body-->
  </div> <!--media-->
</div> <!--panel-->
